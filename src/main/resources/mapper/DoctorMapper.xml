<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.qy.mapper.DoctorMapper">
    <insert id="doctor1Enroll">
        insert into Doctor values(#{name},#{age},#{sex},#{department},#{direction},#{position},'未预约');
    </insert>
    <insert id="addproperties">
        insert into doctorproperties values(#{name},null,null,null,null);
    </insert>
    <update id="update">
        update doctorproperties set registerdName=#{registerdName},cost=#{cost},DateStartTime=#{DateStartTime},DateEndTime=#{DateEndTime} where name=#{doctorName};
    </update>
    <select id="findAll" resultType="com.qy.domain.Doctor">
        select * from doctor
    </select>
    <select id="findByName" resultType="com.qy.domain.Doctor">
        select * from doctor where name=#{name}
    </select>
</mapper>